{
  "name": "example-react-mailer-site",
  "version": "1.0.0",
  "license": "MIT",
  "main": "./dist/webServer.js",
  "description": "React Example Site",
  "repository": "https://github.com/dot1q/example-react-mailer-site.git",
  "scripts": {
    "test": "jest",
    "build": "npm run clean && npm run webpack",
    "webpack": "cross-env NODE_ENV=development node ./node_modules/webpack/bin/webpack --config webpack.dev.js --progress",
    "build-prod": "cross-env NODE_ENV=production node ./node_modules/webpack/bin/webpack --config webpack.prod.js --mode production --progress",
    "build-prod-server": "babel ./src/proxy/index.js --out-file ./dist/webServer.js",
    "coverage": "nyc npm run test",
    "clean": "del-cli dist",
    "serve": "./node_modules/.bin/webpack serve --open --config webpack.dev.js",
    "start": "cross-env SMTP_HOST=mail.example.com SMTP_USER=example@relay.com SMTP_PASS=asdf node ./dist/webServer.js"
  },
